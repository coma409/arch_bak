# Set proxy
alias proxy_on="export all_proxy=http://127.0.0.1:20171"
alias proxy_off="unset all_proxy"
alias git_proxy_on="git config --global http.proxy 'http://127.0.0.1:20171' && git config --global https.proxy 'http://127.0.0.1:20171'"
alias git_proxy_off="git config --global --unset http.proxy && git config --global --unset https.proxy"
alias npm_proxy_on="npm config set proxy http://127.0.0.1:20171 && npm config set https-proxy http://127.0.0.1:20171"
alias npm_proxy_off="npm config delete proxy && npm config delete https-proxy"

# Set zshrc resource
alias sz="source $HOME/.zshrc"

# Set vim & nvim
alias vi="nvim"

# Set cloudreve mount
alias mnt_c="mount /home/coma/Webdav/cloudreve"
alias umnt_c="umount /home/coma/Webdav/cloudreve"

# Set BWG ssh
alias ssh_bwg="ssh -p 26189 root@206.190.238.44"
